<div id="wrapper">
	<div id="page-wrapper">
		<ol class="breadcrumb">
			<li class="breadcrumb-item"><a href="/">Home</a></li>
			<li class="breadcrumb-item">Proof Points</li>
		</ol>
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header"><i class="fa fa-camera-retro fa-fw"></i> Proof Points</h1>
          <div class='pull-right'>
            <p data-placement="top" data-toggle="tooltip" title="Add Proofpoint ">
            <button class="ui right floated btn btn-success btn-sm" data-title="add" data-toggle="modal" data-target="#add"> <span class="glyphicon glyphicon-plus"></span> Add Proofpoint
            </button>
          </div>
			</div>

			<!-- /.col-lg-12 -->
		</div>
		<!-- /.row -->
  	<div class="grid" data-masonry='{ "itemSelector": ".grid-item", "columnWidth": 300 }'>
					{{#each proofpoints}}
			        <div class="col-xs-3 grid-item">
			            <a href="{{this.url}}"><img src="{{this.image}}" /></a>
			        </div>
			    {{/each}}
		</div>
</div>
<script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js"></script>
